
public class StringsManipulation {
	public static void main(String[] args) {
		
		String result = makeOutWord("<<>>", "Gosho");
		System.out.println(result);
	}
	
	public static String makeOutWord(String out, String word) {
		  String frontPart = out.substring(0, out.length() / 2);
		  String backPart = out.substring(out.length() / 2, out.length());
		  
		  return  frontPart + word + backPart;
	}
}
