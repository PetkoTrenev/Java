
public class StringsManipulation {
	public static void main(String[] args) {
		int[] arr = {1, 2, 3};
		System.out.println(maxEnd3(arr));
	}
	
	public static int[] maxEnd3(int[] nums) {
		  int max = nums[0];
		  for (int i = 0; i < nums.length; i++) {
		    if (nums[i] < nums[i+1]) {
		      max = nums[i+1];
		    }
		  }
		  int[] result = {max, max, max};
		  return result;
		}

}
