
public class StringsManipulation {
	public static void main(String[] args) {
		
		lastTwo("coding");
	}
	
	public static String lastTwo(String str) {
		  StringBuilder sb = new StringBuilder();
		  char last = sb.charAt(str.length() - 1);
		  char lastTwo = sb.charAt(str.length() - 2);
		  String word = str.substring(0, str.length()-2);
		  sb.append(word);
		  sb.append(last);
		  sb.append(lastTwo);
		  return sb.toString();
		}

}
